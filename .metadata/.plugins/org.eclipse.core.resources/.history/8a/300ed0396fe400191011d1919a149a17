	 System.out.print("num1-num2单"+(num1-num2));
	 System.out.print("num1*num2单"+(num1*num2));
	 System.out.print("num1/num2单"+(num1/num2));
	 System.out.print("num1%num2单"+(num1%num2));

	 int a = 5;
	 int b = 0;
	 int c = 0;
	 
	 b = a++;
	 c = ++a;
	 System.out.println("]艾Obw趣Й幛~患W,┮Hb涵痊"+b);
